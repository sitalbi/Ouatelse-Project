<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="375.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.s4e2.ouatelse.controllers.StoresMenuController">
    <children>
        <ListView fx:id="storesListView" layoutX="14.0" layoutY="14.0" prefHeight="121.0" prefWidth="175.0" />
        <TextField fx:id="newStoreNameField" alignment="CENTER" layoutX="282.0" layoutY="14.0" promptText="Identifiant du magasin" />
      <TextField fx:id="newStoreManagerField" layoutX="208.0" layoutY="46.0" promptText="Responsable du Magasin" />
      <TextField fx:id="newStoreAddressField" layoutX="364.0" layoutY="46.0" prefHeight="25.0" prefWidth="149.0" promptText="Adresse" />
      <TextField fx:id="newStoreCityField" layoutX="364.0" layoutY="74.0" promptText="Ville" />
      <TextField fx:id="newStoreZipcodeField" layoutX="364.0" layoutY="101.0" promptText="Code postal" />
      <PasswordField fx:id="newStorePasswordField" layoutX="208.0" layoutY="74.0" promptText="Mot de passe" />
      <PasswordField fx:id="newStoreConfirmPasswordField" layoutX="208.0" layoutY="101.0" promptText="Confirmer mot de passe" />
      <Label fx:id="errorMessage" alignment="CENTER" layoutX="199.0" layoutY="126.0" prefHeight="17.0" prefWidth="316.0" textAlignment="CENTER" textFill="#ff0033" wrapText="true" />
        <Button layoutX="282.0" layoutY="143.0" mnemonicParsing="false" onMouseClicked="#onAddButtonClick" prefHeight="25.0" prefWidth="149.0" text="Confirmer" />
      <Button layoutX="418.0" layoutY="336.0" mnemonicParsing="false" onMouseClicked="#onDoneButtonClick" prefHeight="25.0" prefWidth="100.0" text="OK" />
      <Button layoutX="14.0" layoutY="143.0" mnemonicParsing="false" onMouseClicked="#onDeleteButtonClick" prefHeight="25.0" prefWidth="175.0" text="Supprimer" />
    </children>
</AnchorPane>
